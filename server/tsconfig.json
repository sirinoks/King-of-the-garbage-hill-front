{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "resolveJsonModule": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "outDir": "./dist"
    },
    "include": [
      "src/**/*",
      "src/**/*.d.ts"
    ]
}
  